<Window x:Class="PilotkaBotWPF.InventoryMarketWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PilotkaBotWPF"
        mc:Ignorable="d"
        Title="InventoryMarketWindow" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <DataGrid x:Name="dataGridView1" />
            <Button x:Name="buttonItemRequestModeOff" Content="RequestModeOff" Click="buttonItemRequestModeOff_Click_1" />
            <Button x:Name="buttonItemRequestMode" Content="RequestMode" />
            <TextBox x:Name="textBoxPrice" />
            <Label x:Name="labelProgress" ></Label>
        </StackPanel>

    </Grid>
</Window>
